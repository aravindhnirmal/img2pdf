{
    "version": 2,
    "builds": [
        {
            "src": "api.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/api.py"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}
